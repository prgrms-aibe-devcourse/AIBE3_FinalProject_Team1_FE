{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "endOfLine": "auto",
  "quoteProps": "as-needed",
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": false,
  "objectWrap": "preserve",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^next-(.*)$",
    "^next/(.*)$",
    "^@/types/(.*)$",
    "^@/lib/utils/(.*)$",
    "^@/lib/constants/(.*)$",
    "^@/lib/(.*)$",
    "^@/components/ui/(.*)$",
    "^@/components/layout/(.*)$",
    "^@/components/(.*)$",
    "^@/hooks/(.*)$",
    "^@/api/(.*)$",
    "^@/store/(.*)$",
    "^@/queries/(.*)$",
    "^@/services/(.*)$",
    "^lucide-react$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
